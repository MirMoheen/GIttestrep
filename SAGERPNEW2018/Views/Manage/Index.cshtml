<script src="~/Scripts/chartJs/Chart.min.js"></script>
<script>

    $(document).ready(function () {



        var doughnutData = {
            labels: ["Reserved", "Booked", "Available"],
            datasets: [{
                data: ['@ViewBag.SumReserved', '@ViewBag.SumBooked', '@ViewBag.SumAvailable'],
                backgroundColor: ["#e89113", "#c73b3b", "#61ca12"]

            }]
        };


        var doughnutOptions = {
            responsive: true
        };


        var ctx4 = document.getElementById("doughnutChart").getContext("2d");
        new Chart(ctx4, { type: 'doughnut', data: doughnutData, options: doughnutOptions });

        debugger



        var barData = {
            labels: ["F-G", "F-I ", "F-II", "F-III", "T-I", "T-II"],
            datasets: [
                {
                    label: "Total",
                    backgroundColor: 'rgba(208, 147, 86)',
                    pointBorderColor: "#fff",
                    data: ['@ViewBag.totalPlots1', '@ViewBag.totalPlots2', '@ViewBag.totalPlots3', '@ViewBag.totalPlots4', '@ViewBag.totalPlots5', '@ViewBag.totalPlots6']
                },
                {

                    label: "Available",
                    backgroundColor: 'rgba(42, 165, 223)',
                    borderColor: "rgba(26,179,148,0.7)",
                    pointBackgroundColor: "rgba(26,179,148,1)",
                    pointBorderColor: "#fff",
                    data: ['@ViewBag.Available1', '@ViewBag.Available2', '@ViewBag.Available3', '@ViewBag.Available4', '@ViewBag.Available5', '@ViewBag.Available6']
                }
            ]
        };

        var barOptions = {
            responsive: true
        };


        var ctx2 = document.getElementById("barChart").getContext("2d");
        new Chart(ctx2, { type: 'bar', data: barData, options: barOptions });


        var ttt = '@TempData["NORights"]'

        if (ttt != "") {
            $("#basicStop").modal('show');
        }
    })

</script>



